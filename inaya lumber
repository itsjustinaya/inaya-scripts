autobuy:NewSlider("Amount", "how many gifts you want to buy", 100, 0, function(s)
    getgenv().Amount = s
end)
autobuy:NewButton("Autobuy", "Autobuy what you selected", function()
    local success, error = pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Aurora-2004/PuplicProjects/main/AutoBuy"))()
    end)
    if not success then
        warn("Error loading Autobuy script:", error)
    end
end)
-- Misc Section
local Tab = Window:NewTab("misc")
local misc = Tab:NewSection("misc:")
misc:NewButton("Butterhub", "Loads Butterhub", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Butterisgood/Butter/main/Root2.lua'))("")
end)

misc:NewKeybind("Toggle GUI", "Choose the button to toggle the GUI", Enum.KeyCode.F, function()
    Library:ToggleUI()
end)
